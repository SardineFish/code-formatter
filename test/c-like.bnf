<postfix> ::= '++' | '--' | '[' <expr> ']' | '.' 'id' | '->'
<type-ptr-1> ::= '*' <type-ptr-1> | ""
<logic-term-1> ::= <logic-op> <cpr-term> <logic-term-1> | ""
