<block-body> ::= '{' <body> '}' | <statement>
<block> ::= '{' <body> '}'
<const> ::= <number> | 'string' | 'char'
<number> ::= 'number' | 'number-long' | 'number-float'
<expr> ::= <assign>
<assign> ::= <logic-term> <assign-1>
